{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nx-admin-starter/api-auth": ["libs/api/auth/src/index.ts"],
      "@nx-admin-starter/api-core-prisma": [
        "libs/api/core/prisma/src/index.ts"
      ],
      "@nx-admin-starter/api-users": ["libs/api/users/src/index.ts"],
      "@nx-admin-starter/web-users-data-access": [
        "libs/web/users/data-access/src/index.ts"
      ],
      "@nx-admin-starter/web-auth-data-access": [
        "libs/web/auth/data-access/src/index.ts"
      ],
      "@nx-admin-starter/web-auth-feature-login": [
        "libs/web/auth/feature/login/src/index.ts"
      ],
      "@nx-admin-starter/web-auth-feature-shell": [
        "libs/web/auth/feature/shell/src/index.ts"
      ],
      "@nx-admin-starter/web-dashboard-feature": [
        "libs/web/dashboard/feature/src/index.ts"
      ],
      "@nx-admin-starter/web-settings-feature": [
        "libs/web/settings/feature/src/index.ts"
      ],
      "@nx-admin-starter/web-shell-feature": [
        "libs/web/shell/feature/src/index.ts"
      ],
      "@nx-admin-starter/web-shell-ui": ["libs/web/shell/ui/src/index.ts"],
      "@nx-admin-starter/web-users-feature": [
        "libs/web/users/feature/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
